<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_set"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.nammu.smartwifi.util.setdata.SetActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/set_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/toolbar_draw" >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawingCacheQuality="auto"
            android:layout_gravity="right"
            android:id="@+id/iv_toolbar"
            android:layout_marginRight="10dp"
            android:visibility="gone"
            app:srcCompat="@drawable/arrows" />
    </android.support.v7.widget.Toolbar>
    <fragment
        android:id="@+id/fragment_set"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:name="com.nammu.smartwifi.util.setdata.fragment.SetFragment"
        android:layout_weight="1"/>

</LinearLayout>
